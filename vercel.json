{
    "version": 2,
    "builds": [
      {
        "src": "Dockerfile",
        "use": "@vercel/docker"
      }
    ],
    "routes": [
      { "handle": "filesystem" },
      {
        "src": "/(.*)",
        "dest": "app.wsgi:application",
        "methods": ["GET", "HEAD", "OPTIONS", "POST", "PUT", "DELETE"]
      }
    ]
  }
  